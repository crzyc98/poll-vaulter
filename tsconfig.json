{
    "compilerOptions": {
        "module": "NodeNext",
        "target": "ESNext",
        "outDir": "dist",
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "isolatedModules": false,
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "sourceMap": true
    },
    "extends": "@tsconfig/node16/tsconfig.json",
    "include": [
        "src"
    ]
}
